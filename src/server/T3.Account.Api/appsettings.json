{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "redis://localhost:6379"
  },
  "AccountTokenSettings": {
    "Issuer": "http://localhost:5189"
  },
  "WebTokenSettings": {
    "PrivateKeyPath": "jwt.private",
    "PublicKeyPath": "jwt.public"
  },
  "PasswordV1Settings": {
    "SaltSize": 16,
    "HashSize": 20,
    "Iterations": 10000
  }
}
